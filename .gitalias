[alias]
        a = add

        amend = commit --amend

        b = branch

        c = commit

        co = checkout

        d = diff --color

        g = !git gui

        k = !gitk --all

        lc = !git ol ORIG_HEAD.. --stat --no-merges

        m = merge --no-ff
        mff = merge

        ol = log --pretty=oneline --abbrev-commit --graph

        p = push

        pl = pull

        rb = rebase

        s = status

        st  = stash
        stl = stash list
        sta = stash apply
        std = stash drop
        sts = stash show -p
        stp = stash pop

        unc = reset --soft HEAD^

        who = shortlog -sne
